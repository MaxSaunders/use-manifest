    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "jest": "^24.9.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "env": [
      "jest"
